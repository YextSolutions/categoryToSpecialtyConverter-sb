{
  "$id": "categoryToSpecialtyConverter",
  "$schema": "https://schema.yext.com/config/km/connector/v3",
  "displayName": "Category to Specialty Converter",
  "entityType": "healthcareProfessional",
  "source": {
    "sourceConfig": {
      "nativeSourceConfig": {
        "source": "yextAccount",
        "operation": "fetchEntities",
        "inputs": {
          "apiKey": "${{apiKey}}",
          "entityType": "healthcareProfessional",
          "environment": "sandbox",
          "vParam": "20230531"
        }
      }
    },
    "selectors": [
      {
        "type": "JSON",
        "header": "Entity ID",
        "path": "meta.id"
      },
      {
        "type": "JSON",
        "header": "Related Specialties",
        "path": "categoryIds"
      }
    ]
  },
  "transforms": [
    {
      "function": {
        "inputHeaders": {
          "headers": [
            "Related Specialties"
          ]
        },
        "plugin": "categoryToSpecialty",
        "function": "sandboxCategoryToSpecialty"
      }
    }
  ],
  "mappings": [
    {
      "header": "Related Specialties",
      "field": "taxonomy_relatedSpecialties",
      "subfieldPath": [],
      "clearValueIfBlank": false
    },
    {
      "header": "Entity ID",
      "field": "entityId",
      "subfieldPath": [],
      "clearValueIfBlank": false
    }
  ],
  "comprehensiveModeConfig": {
    "comprehensiveSet": "ENTITIES_CREATED_BY_THIS_CONNECTOR",
    "actionOnMissingEntities": "DELETE"
  }
}